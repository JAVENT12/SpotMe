@model IEnumerable<MuscleGroup>
@*<div><h3>Muscle Groups</h3></div>*@
<div class="text-danger" asp-validation-summary="ModelOnly"></div>


<table class="table table-striped table-bordered table-sm">
    <tr>
        <th class="text-center">Muscles</th>

    </tr>
    @if (Model.Count() == 0)
    {
        <tr><td colspan="4" class="text-center">No Muscles</td></tr>
    }
    else
    {
        foreach (var role in Model)
        {
            <tr>
                <td colspan="4" class="text-center">@role.name</td>
            </tr>
        }
    }
</table>
